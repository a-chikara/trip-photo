<div class="post_info">
  <% @posts.each do |post| %>
    <p><%= post.city.name %></p>
    <%= l post.created_at %>
    <%= link_to image_tag(post.image.variant(resize: '500x500'), class: :post_image), "#" %>

    <p><%= post.explain %></p>
        
    <%= link_to '削除', post_path(post.id), method: :delete %>
  <% end %>
</div>